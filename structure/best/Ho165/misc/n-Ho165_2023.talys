#
# General
#
ldmodel 2
#
# (n,tot), (n,el), (n,inl)
#
#
# (n,p), (n,2n), (n,np)
#
rvadjust p 0.97
avadjust p 0.97
#
# (n,a)
#
rvadjust a 1.07
avadjust a 1.07
cknock a 0.35
cstrip a 0.35
#
# (n,g)
ldmodelCN 1 
#
pshiftadjust 67 165 0.80
#
# Other: Isomers, (n,d), (n,t), (n,h) etc.
#
rvadjust t 1.10
cknock t 2.3
cstrip t 2.3
cknock h 3.3
cstrip h 3.3
ngfit y
